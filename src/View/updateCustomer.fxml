<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.ComboBox?>
<AnchorPane prefHeight="421.0" prefWidth="340.0" style="-fx-background-color: White" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.updateCustomer">
    <children>
        <Label layoutX="20.0" layoutY="20.0" prefHeight="45.0" prefWidth="330.0" text="Update Customer View">
            <font>
                <Font name="System Bold" size="30.0" />
            </font>
        </Label>
        <VBox layoutX="185.0" layoutY="64.0" prefWidth="150.0" spacing="16.0">
            <children>
                <TextField fx:id="id" disable="true" />
                <TextField fx:id="name" />
                <TextField fx:id="address" />
                <ComboBox fx:id="country" onAction="#divBasedOnCntry" prefWidth="150.0" />
                <ComboBox fx:id="division" prefWidth="150.0" />
                <TextField fx:id="postal" />
                <TextField fx:id="phone" />
            </children>
        </VBox>
        <VBox alignment="CENTER_RIGHT" layoutX="34.0" layoutY="65.0" prefHeight="200.0" prefWidth="121.0" spacing="25.0">
            <children>
                <Label prefHeight="25.0" text="Customer_ID:">
                    <font>
                        <Font name="System Bold" size="12.0" />
                    </font>
                </Label>
                <Label prefHeight="25.0" text="Customer_Name:">
                    <font>
                        <Font name="System Bold" size="12.0" />
                    </font>
                </Label>
                <Label prefHeight="25.0" text="Address:">
                    <font>
                        <Font name="System Bold" size="12.0" />
                    </font>
                </Label>
                <Label prefHeight="25.0" text="Country:">
                    <font>
                        <Font name="System Bold" size="12.0" />
                    </font>
                </Label>
                <Label prefHeight="25.0" text="Division:">
                    <font>
                        <Font name="System Bold" size="12.0" />
                    </font>
                </Label>
                <Label prefHeight="25.0" text="Postal_Code:">
                    <font>
                        <Font name="System Bold" size="12.0" />
                    </font>
                </Label>
                <Label prefHeight="25.0" text="Phone:">
                    <font>
                        <Font name="System Bold" size="12.0" />
                    </font>
                </Label>
            </children>
        </VBox>
        <Button layoutY="349.0" mnemonicParsing="false" onAction="#updateCustomer" prefHeight="40.0" prefWidth="150.0" text="Save">
            <font>
                <Font size="14.0" />
            </font>
        </Button>
        <Button layoutX="200.0" layoutY="349.0" mnemonicParsing="false" onAction="#cancel" prefHeight="40.0" prefWidth="150.0" text="Cancel">
            <font>
                <Font size="14.0" />
            </font>
        </Button>
    </children>
</AnchorPane>
